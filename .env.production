# Production environment variables for frontend

# FastAPI Backend API URL (на том же сервере, порт 8123)
VITE_BACKEND_URL=/api

# Socket.IO Game Server (используй относительный путь /game для Nginx proxy)
VITE_GAME_WS_URL=/socket.io
VITE_GAME_NAMESPACE=/game

# TGS stickers URL mapped via Nginx to avoid CORS
# With the provided Nginx config, frontend will request /tgs/<slug>.tgs (same-origin)
VITE_S3_GIFTS_URL=
VITE_S3_GIFTS_PATH=/tgs
